      <TextField fx:id="MemoryLimit" layoutX="152.0" layoutY="336.0" />
      <Label layoutX="14.0" layoutY="336.0" prefHeight="31.0" prefWidth="128.0" text="Memory Limit(kb):" textFill="WHITE" />
      <Button layoutX="307.0" layoutY="401.0" mnemonicParsing="false" onAction="#SubmitButtonClicked" text="Submit" />
      <Label fx:id="ErrorMessege" layoutX="14.0" layoutY="401.0" prefHeight="31.0" prefWidth="285.0" text="Error Messege" textFill="WHITE" />
      <Button layoutX="14.0" layoutY="107.0" mnemonicParsing="false" onAction="#AddInput" text="Add Input -&gt; Output" />
   </children>
</AnchorPane>
